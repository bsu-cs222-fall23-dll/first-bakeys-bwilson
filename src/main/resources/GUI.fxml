<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>
<VBox alignment="CENTER" prefWidth="262.0" spacing="12.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WikipediaRevisionHistory.GUI.Controller">
   <Label text="Wikipedia revision history">
      <VBox.margin>
         <Insets bottom="18.0" />
      </VBox.margin>
   </Label>
   <HBox alignment="CENTER" spacing="4.0">
      <Label text="URL:" />
      <TextField fx:id="userInput" alignment="CENTER"/>
   </HBox>
   <Button mnemonicParsing="false" onAction="#search" text="Search"/>
   <TextFlow fx:id="resultTextFlow" textAlignment="CENTER"/>
   <padding>
      <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
   </padding>
</VBox>
